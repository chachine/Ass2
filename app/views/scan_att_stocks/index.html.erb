<p id="notice"><%= notice %></p>

<h1>Scan Att Stocks</h1>

<table>
  <thead>
    <tr>

      <th>Type</th>
      <th>Qty</th>
      <th>Du</th>
      <th>Au</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scan_att_stocks.each do |scan_att_stock| %>
      <tr>

        <td><%= scan_att_stock.typeV %></td>
        <td><%= scan_att_stock.qty %></td>
        <td><%= scan_att_stock.du %></td>
        <td><%= scan_att_stock.au %></td>
        <td><%= link_to 'Show', scan_att_stock %></td>
        <td><%= link_to 'Edit', edit_scan_att_stock_path(scan_att_stock) %></td>
        <td><%= link_to 'Destroy', scan_att_stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

