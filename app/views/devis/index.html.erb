<p id="notice"><%= notice %></p>

<h1>Devis</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @devis.each do |devi| %>
      <tr>
        <td><%= link_to 'Show', devi %></td>
        <td><%= link_to 'Edit', edit_devi_path(devi) %></td>
        <td><%= link_to 'Destroy', devi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Devi', new_devi_path %>
